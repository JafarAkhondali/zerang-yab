{
  "name": "ZerangYab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "ZerangYab -Twitter UnFollowers finder",
  "homepage_url": "https://github.com/JafarAkhondali/zerang-yab",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "src/background.js"
    ]
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo"
  },
  "permissions": [
    "activeTab",
    "webRequestBlocking",
    "webRequest",
    "tabs",
    "https://twitter.com/*"
  ]
}